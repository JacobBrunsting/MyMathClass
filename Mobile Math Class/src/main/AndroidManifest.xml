<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.brunsting.jacob.mobilemathclass">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">
        <activity android:name="com.brunsting.jacob.mobilemathclass.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.brunsting.jacob.mobilemathclass.QuizCreation" />
        <activity android:name="com.brunsting.jacob.mobilemathclass.DifficultySelect" />
        <activity android:name="com.brunsting.jacob.mobilemathclass.QuizQuestions" />
        <activity android:name="com.brunsting.jacob.mobilemathclass.QuizOver" />
        <activity android:name="com.brunsting.jacob.mobilemathclass.IncorrectQuestionViewer" />
        <activity android:name="com.brunsting.jacob.mobilemathclass.QuizSettings"></activity>
    </application>

</manifest>
